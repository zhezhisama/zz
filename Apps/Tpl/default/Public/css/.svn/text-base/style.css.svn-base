/*footer*/
.footer{border-top: 1px solid #CCCCCC; padding:30px 0 0; color: #808080; font-size: 14px; height: auto; line-height: 20px; text-align: center; width: 100%;}
.footer .foot{ width:960px;}
.footer .foot ul { width:255px; float:left; overflow:hidden;}
.footer .foot ul p{font-size: 10px; padding:20px}
.footer .foot ul.footmid{ width:320px; padding:10px 0 10px 20px}
.foot .footmid a.gohome, .foot .footmid a.setting, .foot .footmid a.goout{width:77px; height:78px; float:left; margin:0 10px;}
.foot .footmid a.gohome{ background:url(../images/footer_bt.png) no-repeat 0 0; }
.foot .footmid a.gohome:hover{ background:url(../images/footer_bt.png) no-repeat 0 -79px;}
.foot .footmid a.setting{ background:url(../images/footer_bt.png) no-repeat -102px 0;}
.foot .footmid a.setting:hover{ background:url(../images/footer_bt.png) no-repeat -102px -79px;}
.foot .footmid a.goout{ background:url(../images/footer_bt.png) no-repeat right 0;}
.foot .footmid a.goout:hover{ background:url(../images/footer_bt.png) no-repeat right -79px;}
.foot ul p.addr{ float:left;font-family: "Comic Sans MS",cursive; line-height: 24px; text-align: center; padding:10px 0}
